<div id="label-area">
  <div class="btn btn-lg btn-primary" id="btn-class-1">1</div>
  <div class="btn btn-lg btn-success" id="btn-class-2">2</div>
  <div class="btn btn-lg btn-danger"  id="btn-class-3">3</div>
  <div class="btn btn-lg btn-default" id="btn-class-0">0</div>
</div>

<form>
  <div class="btn btn-lg btn-warning" id="send-labels">送信</div>
  <div class="btn btn-lg btn-warning" id="decide-labels">確定!</div>
</form>

<div class="center jumbotron">
  <ul id="gallery">
    <% @images.each do |image| %>
    <li class="image-item" id="image-<%= image.id %>"><%= image_tag(image.url) %></li>
    <% end %>
  </ul>
</div>

<div id="user-info-storage" data-user-email='<%= current_user.email %>'><div>
<div id="log-storage" data-log='[]'></div>
